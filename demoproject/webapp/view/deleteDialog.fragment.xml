<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" >
    <Dialog type="Message" title="Delete">
        <Text text="Are you sure you want to delete this?"/>
        <beginButton>
             <Button text="Confirm" press="onDeleteConfirm"/>
        </beginButton>
        <endButton>
            <Button text="Cancel" press="onDeleteCancel"/>
        </endButton>
    </Dialog>
</core:FragmentDefinition>